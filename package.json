{
  "name": "zerodha-trading-platform",
  "version": "1.0.0",
  "description": "A full-stack trading platform clone built with React.js and Node.js",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start-backend": "cd backend && npm run dev",
    "start-frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "build": "cd frontend && npm run build",
    "build-backend": "cd backend && npm install --production",
    "build-all": "npm run build-backend && npm run build",
    "start": "cd backend && npm run prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "preview": "cd frontend && npm run preview",
    "vercel-build": "npm run build-all"
  },
  "keywords": [
    "trading",
    "stock-market",
    "react",
    "nodejs",
    "mongodb",
    "zerodha"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
    "vite": "^7.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.6.3",
    "react-toastify": "^11.0.5"
  }
}
